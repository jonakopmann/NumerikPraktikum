#!/usr/bin/gnuplot
set terminal pngcairo enhanced
set xrange [0:512]
set yrange [0:509]

set cblabel "Intensity"
set xtics nomirror
set ytics nomirror
set xtics out
set ytics out
set xlabel 'Position/Pixel'
set ylabel 'Hight/Pixel'

set title "Test"
set out "test.png"
plot "out.txt" matrix with image